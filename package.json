{
  "name": "spaceweb",
  "version": "0.0.15",
  "description": "Components for SpaceWeb",
  "main": "index.js",
  "module": "./esm/index.js",
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build:package-json": "shx cp package.json dist/package.json",
    "build:cjs": "tsc --module commonjs --outDir ./dist",
    "build:esm": "tsc --module es2015 --outDir ./dist/esm",
    "build": "yarn prebuild && yarn build:cjs && yarn build:esm && yarn build:package-json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sprinklr/style-dictionary": "^2.2.1",
    "baseui": "^9.54.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/parser": "^7.7.3",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/traverse": "^7.7.0",
    "@babel/types": "^7.7.1",
    "@sprinklr/types": "^3.0.0",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-plugin-transform-styletron-display-name": "^1.1.3",
    "cross-env": "^6.0.0",
    "shx": "^0.3.2"
  }
}
